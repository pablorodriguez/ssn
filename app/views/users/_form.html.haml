= simple_form_for(@user,html: { class: 'form-horizontal' } ) do |f|
  = f.input :first_name
  = f.input :last_name
  = f.input :email
  = f.input :cellphone
  = f.input :address
  = f.input :city
  = f.input :state
  .form-group.row
    .col-md-9.offset-md-3
      = f.submit 'Save', class: "btn btn-primary"
